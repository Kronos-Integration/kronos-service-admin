{
  "name": "kronos-service-admin",
  "version": "0.0.0-semantic-release",
  "description": "kronos server manager admin service",
  "keywords": [
    "kronos-service"
  ],
  "main": "dist/service-admin.js",
  "module": "src/service-admin.js",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "semantic-release": "semantic-release",
    "docs": "jsdoc2md --configure doc/jsdoc.json -l off -t doc/README.hbs -f src/*.js >README.md",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "precover": "rollup -c tests/rollup.config.js",
    "prepare": "rollup -c"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-preset-env": "^1.6.1",
    "got": "^8.0.1",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-to-markdown": "^3.0.2",
    "kronos-service-manager": "3.5.10",
    "kronos-service-registry": "1.4.9",
    "kronos-step-stdio": "^3.0.18",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.4.0",
    "rollup": "^0.52.1",
    "rollup-plugin-multi-entry": "^2.0.2",
    "semantic-release": "^11.0.2",
    "xo": "^0.19.0"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "kronos-flow": "^2.5.38",
    "kronos-endpoint": "2.14.14",
    "kronos-flow-control-step": "^3.0.11",
    "kronos-http-routing-step": "^4.3.24",
    "kronos-interceptor-decode-json": "^1.0.12",
    "kronos-interceptor-http-request": "^3.2.10",
    "kronos-service": "4.13.1",
    "kronos-service-health-check": "2.10.11",
    "kronos-service-koa": "^3.3.15",
    "kronos-step-aggregate": "^1.4.6",
    "model-attributes": "^3.0.10",
    "ssh2": "^0.5.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kronos-Integration/kronos-service-admin.git"
  },
  "engines": {
    "node": ">=8.9.3"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service-admin/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service-admin#readme",
  "nyc": {
    "include": [
      "build/*-test.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  },
  "xo": {
    "space": true
  },
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  }
}
