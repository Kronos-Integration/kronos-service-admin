{
  "name": "kronos-service-admin",
  "version": "0.0.0-semantic-release",
  "description": "kronos server manager admin service",
  "keywords": [
    "kronos-service"
  ],
  "main": "dist/module.js",
  "module": "src/ServiceAdmin.js",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc npm test",
    "test": "mocha tests/*-test.js",
    "semantic-release": "semantic-release",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "pretest": "rollup -c",
    "posttest": "markdown-doctest",
    "precover": "rollup -c",
    "prepare": "rollup -c"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "jsdoc-to-markdown": "^3.0.2",
    "kronos-service-manager": "3.5.10",
    "kronos-service-registry": "1.4.9",
    "kronos-step-stdio": "^3.0.18",
    "markdown-doctest": "^0.9.1",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "rollup": "^0.51.5",
    "semantic-release": "^9.1.1"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "kronos-endpoint": "2.14.11",
    "kronos-flow": "^2.5.34",
    "kronos-flow-control-step": "^3.0.11",
    "kronos-http-routing-step": "^4.3.24",
    "kronos-interceptor-decode-json": "^1.0.12",
    "kronos-interceptor-http-request": "^3.2.9",
    "kronos-service": "4.8.2",
    "kronos-service-health-check": "2.10.11",
    "kronos-service-koa": "^3.3.11",
    "kronos-step-aggregate": "^1.4.6",
    "model-attributes": "^3.0.10",
    "ssh2": "^0.5.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kronos-Integration/kronos-service-admin.git"
  },
  "engines": {
    "node": ">=8.9.1"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service-admin/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service-admin#readme",
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/Kronos-Tools/npm-package-template.git"
    }
  },
  "xo": {
    "space": true
  }
}
