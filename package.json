{
  "name": "kronos-service-admin",
  "version": "0.0.0-semantic-release",
  "description": "kronos server manager admin service",
  "keywords": [
    "kronos-service"
  ],
  "main": "dist/module.js",
  "module": "src/ServiceAdmin.js",
  "scripts": {
    "cover": "nyc npm test",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "prepublish": "rollup --output=dist/module.js -c -- src/ServiceAdmin.js",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest",
    "precover": "npm run prepublish"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "cz-conventional-changelog": "^1.2.0",
    "jsdoc-to-markdown": "^2.0.1",
    "kronos-service-manager": "^3.5.5",
    "kronos-service-registry": "^1.4.3",
    "kronos-step-stdio": "^3.0.18",
    "markdown-doctest": "^0.9.1",
    "mocha": "^3.2.0",
    "rollup": "^0.41.4",
    "semantic-release": "^6.3.6",
    "nyc": "^10.0.0"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "kronos-endpoint": "^2.14.8",
    "kronos-flow": "^2.5.34",
    "kronos-flow-control-step": "^3.0.11",
    "kronos-http-routing-step": "^4.3.24",
    "kronos-interceptor-decode-json": "^1.0.8",
    "kronos-interceptor-http-request": "^3.2.3",
    "kronos-service": "^4.5.1",
    "kronos-service-health-check": "^2.10.3",
    "kronos-service-koa": "^3.3.1",
    "kronos-step-aggregate": "^1.4.6",
    "model-attributes": "^3.0.3",
    "ssh2": "^0.5.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-service-admin.git"
  },
  "engines": {
    "node": ">=6.9.4"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service-admin/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service-admin#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  }
}