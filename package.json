{
  "name": "@kronos-integration/service-admin",
  "version": "0.0.0-semantic-release",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/service-admin.mjs",
  "module": "src/service-admin.mjs",
  "description": "kronos server manager admin service",
  "keywords": [
    "kronos-service"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/service-admin.mjs --section=API",
    "lint": "documentation lint src/service-admin.mjs",
    "posttest": "markdown-doctest",
    "test": "ava"
  },
  "dependencies": {
    "@kronos-integration/endpoint": "*",
    "@kronos-integration/service": "^6.0.1",
    "@kronos-integration/service-health-check": ">=3.0.7",
    "model-attributes": "^4.1.0",
    "ssh2": "^0.8.6"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "builtin-modules": "^3.1.0",
    "c8": "^6.0.1",
    "documentation": "^12.1.3",
    "esm": "^3.2.25",
    "got": "^9.6.0",
    "markdown-doctest": "^1.0.0",
    "semantic-release": "^15.13.30"
  },
  "engines": {
    "node": ">=12.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-service-admin.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service-admin/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service-admin#readme",
  "ava": {
    "files": [
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  }
}
