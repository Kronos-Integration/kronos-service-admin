{
  "name": "@kronos-integration/service-admin",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/service-admin.mjs",
  "exports": {
    ".": "./src/service-admin.mjs"
  },
  "description": "kronos server manager admin service",
  "keywords": [
    "kronos-service"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/service-admin.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/service-admin.mjs"
  },
  "dependencies": {
    "@kronos-integration/service": "^9.2.15"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "c8": "^7.3.1",
    "documentation": "^13.0.2",
    "got": "^11.6.0",
    "semantic-release": "^17.1.2"
  },
  "engines": {
    "node": ">=14.13.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/service-admin.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/service-admin/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/service-admin#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-github-action",
      "arlac77/template-kronos-component"
    ]
  }
}
