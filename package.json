{
  "name": "kronos-service-admin",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/service-admin.js",
  "module": "src/service-admin.js",
  "description": "kronos server manager admin service",
  "keywords": [
    "kronos-service"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "semantic-release": "semantic-release",
    "docs": "documentation readme src/service-admin.js --section=API",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "precover": "rollup -c tests/rollup.config.js",
    "prepare": "rollup -c",
    "lint": "documentation lint src/service-admin.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "kronos-flow": "^2.5.40",
    "kronos-endpoint": "2.15.0",
    "kronos-flow-control-step": "^3.0.11",
    "kronos-http-routing-step": "^4.3.24",
    "kronos-interceptor-decode-json": "^1.1.0",
    "kronos-interceptor-http-request": "^3.2.10",
    "kronos-service": "4.17.0",
    "kronos-service-health-check": "2.11.0",
    "kronos-service-koa": "^4.0.6",
    "kronos-step-aggregate": "^1.4.6",
    "model-attributes": "^3.0.10",
    "ssh2": "^0.6.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "got": "^9.2.1",
    "documentation": "^8.1.2",
    "kronos-service-manager": "3.5.10",
    "kronos-service-registry": "1.5.0",
    "kronos-step-stdio": "^3.0.18",
    "markdown-doctest": "^0.9.1",
    "nyc": "^13.0.1",
    "rollup": "^0.65.2",
    "rollup-plugin-multi-entry": "^2.0.2",
    "semantic-release": "^15.9.13",
    "travis-deploy-once": "^5.0.7",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-cleanup": "^3.0.0"
  },
  "engines": {
    "node": ">=10.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-service-admin.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-service-admin/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-service-admin#readme",
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
